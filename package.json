{
  "name": "tutorly",
  "version": "1.0.0",
  "description": "\"A web application where tutors and students meet\"",
  "main": "server.js",
  "engines": {
    "node": "16.17.1"
  },
  "scripts": {
    "start": "npm run build && node dist/server.js",
    "build": "babel src -d dist",
    "dev": "env-cmd -f ./src/config/.dev.env nodemon --ignore ./client/ --exec babel-node ./src/server.js",
    "prod-dev": "env-cmd -f ./src/config/.prod.env nodemon --ignore ./client/ --exec babel-node ./src/server.js",
    "client": "npm start --prefix client",
    "serve-app": "concurrently \"npm run dev\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Degraphe",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/node": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@sendgrid/mail": "^7.7.0",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "cloudinary": "^1.23.0",
    "concurrently": "^6.0.0",
    "datauri": "^3.0.0",
    "env-cmd": "10.1.0",
    "express": "4.17.1",
    "express-fileupload": "^1.4.0",
    "express-validator": "^6.11.1",
    "googleapis": "^105.0.0",
    "gravatar": "^1.8.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "mongoose": "^5.12.7",
    "mongoose-paginate-v2": "^1.4.2",
    "multer": "^1.4.2",
    "nodemailer": "^6.7.1",
    "nodemon": "^2.0.20",
    "peerjs": "^1.3.2",
    "react-slick": "^0.29.0",
    "sass": "^1.58.0",
    "sharp": "^0.28.3",
    "socket.io": "^4.5.4",
    "stripe": "^12.3.0",
    "uuid": "^8.3.2"
  }
}
